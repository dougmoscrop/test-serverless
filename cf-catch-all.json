{
	"AWSTemplateFormatVersion": "2010-09-09",
	"Resources": {
		"HttpApi": {
			"Type": "AWS::ApiGatewayV2::Api",
			"Properties": {
				"Name": "cloud-formation-test-catch-all",
				"Description": "Test from CloudFormation",
				"ProtocolType": "HTTP",
				"RouteKey": "$default",
				"Target": "arn:aws:lambda:us-east-1:992311060759:function:test-http-api-dev-function"
			}
		},

		"Stage": {
			"Type": "AWS::ApiGatewayV2::Stage",
			"Properties": { "ApiId": { "Ref": "HttpApi" }, "StageName": "dev", "AutoDeploy": true }
		}
	}
}
