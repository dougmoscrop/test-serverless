{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "The AWS CloudFormation template for this Serverless application",
  "Resources": {
    "ServerlessDeploymentBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketEncryption": {
          "ServerSideEncryptionConfiguration": [
            {
              "ServerSideEncryptionByDefault": {
                "SSEAlgorithm": "AES256"
              }
            }
          ]
        }
      }
    },
    "MyFunctionLogGroup": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/aws/lambda/test-custom-role-lambda-dev-myFunction"
      }
    },
    "MyFunctionLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "ServerlessDeploymentBucket"
          },
          "S3Key": "serverless/test-custom-role-lambda/dev/1566894028986-2019-08-27T08:20:28.986Z/myFunction.zip"
        },
        "FunctionName": "test-custom-role-lambda-dev-myFunction",
        "Handler": "index.handler",
        "MemorySize": 1024,
        "Role": "arn:aws:iam::992311060759:role/test-custom-role",
        "Runtime": "nodejs10.x",
        "Timeout": 6
      },
      "DependsOn": [
        "MyFunctionLogGroup"
      ]
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": {
        "Ref": "ServerlessDeploymentBucket"
      }
    }
  }
}